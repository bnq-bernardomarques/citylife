package com.feedzai.testServices;

import com.feedzai.jervicectl.Service;

public class AnotherService implements Service{

    public boolean start(){
        System.out.println("AnotherService::start() ==> start");
        try{
            Thread.sleep(1500);
        }catch(InterruptedException e){
            e.printStackTrace();
        }
        System.out.println("AnotherService::start() ==> end");
	return true;
    }

    public void stop(){
        System.out.println("AnotherService::stop() ==> start");
        try{
            Thread.sleep(1500);
        }catch(InterruptedException e){
            e.printStackTrace();
        }
        System.out.println("AnotherService::stop() ==> end");
    }

}
